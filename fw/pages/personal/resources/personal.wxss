}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
  overflow: hidden;
  margin: 0 auto;
  display: block;
}

.userinfo-nickname {
  color: #333;
  font-size: 16px;
  display: block;
  margin-top: 16rpx;
}

